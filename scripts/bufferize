#!/bin/bash

PASSES="-arith-bufferize -standalone-bufferize -tensor-bufferize -linalg-bufferize -scf-bufferize -func-bufferize -finalizing-bufferize -buffer-hoisting -buffer-loop-hoisting -standalone-loop-hoisting -promote-buffers-to-stack -buffer-deallocation"
